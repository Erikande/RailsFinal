 <h1>Dashboard</h1><br>

 <h4>Your locations:</h4>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Address</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
  <% @places.each do |x| %><br>
      <tr>
        <td><%= x.address %><br></td>
        <td>
          <%= link_to 'Show', page_path(x), :class => 'btn btn-mini btn-danger' %>
          <%= link_to 'Edit', edit_inside_path(x), :class => 'btn btn-mini btn-danger' %>
          <%= link_to 'Delete', page_path(x), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_inside_path, :class => 'btn btn-primary' %>




